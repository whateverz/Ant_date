<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="ThemeActivity" mce_bogus="1" parent="AppTheme">
        <item name="android:windowBackground">@color/default_bg</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="android:windowAnimationStyle">@style/AnimationActivity</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppFullTheme" parent="AppTheme">
        <!-- Customize your theme here. -->
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="ThemeStartActivity" mce_bogus="1" parent="ThemeActivity">
        <item name="android:windowBackground">@color/theme_color</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="ThemeFullScrActivity" mce_bogus="1" parent="ThemeActivity">

    </style>

    <style name="ThemeFullScrBlack" parent="Theme.AppCompat.NoActionBar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/black</item>
    </style>

    <style name="AppTheme.Dialog.Alert" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">@color/theme_color_orange</item>
    </style>

    <style name="ThemeTranslucentActivity" mce_bogus="1" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="AnimationActivity" mce_bogus="1" parent="android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/push_up_in</item>
        <item name="android:activityOpenExitAnimation">@anim/push_up_out</item>
        <item name="android:activityCloseEnterAnimation">@anim/pull_up_in</item>
        <item name="android:activityCloseExitAnimation">@anim/pull_up_out</item>
    </style>

    <style name="DlgMenu" parent="android:Animation">
        <item name="android:windowExitAnimation">@anim/fade_out</item>
        <item name="android:windowEnterAnimation">@anim/fade_in</item>
    </style>

    <style name="DlgPopup" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/popup_dl_in</item>
        <item name="android:windowExitAnimation">@anim/popup_dl_out</item>
    </style>

    <style name="DlgSlipOut" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/slide_in_bottom</item>
        <item name="android:windowExitAnimation">@anim/slide_out_bottom</item>
    </style>

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="dialog_pb" parent="@android:style/Theme.Holo.Dialog">
        <item name="android:windowFrame">@null</item>
        <!-- 边框 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">false</item>
        <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- 背景透明 -->
        <item name="android:backgroundDimEnabled">false</item>
        <!-- 模糊 -->
        <item name="android:background">#00000000</item>
    </style>

    <style name="WkBaseText">
        <item name="android:textColor">@color/wg_actionbar_text</item>
        <item name="android:textSize">@dimen/wk_base_text_size</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="ActionBarTextView" parent="@style/WkBaseText">
        <item name="android:textColor">@color/new_text_actionbar</item>
        <item name="android:textSize">@dimen/new_text_34px</item>
    </style>


    <style name="TextNormalWrite">
        <item name="android:textColor">#ffffffff</item>
        <item name="android:textSize">@dimen/new_text_24px</item>
    </style>

    <style name="TabButton">
