<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    >
    <ImageView
        android:id="@+id/iv_back"
        android:layout_width="@dimen/new_60px"
        android:layout_height="@dimen/new_60px"
        android:src="@mipmap/icon_back"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="@dimen/new_124px"
        android:layout_marginLeft="@dimen/new_68px"
        tools:ignore="MissingConstraints" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="@dimen/new_247px"
        android:textSize="@dimen/new_66px"
        android:text="注册"
        tools:ignore="MissingConstraints"
        android:layout_marginLeft="@dimen/new_137px"
        />


    <ImageView
        android:layout_width="@dimen/new_36px"
        android:layout_height="@dimen/new_48px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:src="@mipmap/icon_pass"
        android:layout_marginTop="@dimen/new_635px"
        tools:ignore="MissingConstraints"
        android:layout_marginLeft="@dimen/new_137px"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="@dimen/new_65px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="@dimen/new_635px"
        android:textSize="@dimen/new_48px"
        android:text="设置新密码"
        tools:ignore="MissingConstraints"
        android:layout_marginLeft="@dimen/new_203px"
        />

    <EditText
        android:id="@+id/et_pass"
        android:layout_width="@dimen/new_806px"
        android:layout_height="@dimen/new_65px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="@dimen/new_754px"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="@dimen/new_137px"
        android:hint="请设置密码"
        android:inputType="textPassword"
        android:maxLength="8"
        tools:ignore="MissingConstraints"
        android:background="@color/white"
        android:layout_marginLeft="@dimen/new_137px"
        />
    <ImageView
        android:id="@+id/iv_eyes"
        android:layout_width="@dimen/new_39px"
        android:layout_height="@dimen/new_39px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="@dimen/new_764px"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="@dimen/new_137px"
        tools:ignore="MissingConstraints"
        android:src="@mipmap/icon_cloeye"
        android:layout_marginLeft="@dimen/new_904px"
        />
    <ImageView
        android:layout_width="@dimen/new_806px"
        android:layout_height="@dimen/new_1px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="@dimen/new_824px"
        android:background="@color/a_main_tab_normal"
        tools:ignore="MissingConstraints"
        android:layout_marginRight="@dimen/new_137px"
        android:layout_marginLeft="@dimen/new_137px"
        />
    <TextView
        android:id="@+id/tv_alert"
        android:layout_width="@dimen/new_468px"
        android:layout_height="@dimen/new_54px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="@dimen/new_768px"
        tools:ignore="MissingConstraints"
        android:text="验证码已超时，请重新获取"
        android:visibility="invisible"
        android:textSize="@dimen/negative_39px"
        android:textColor="@color/tv_alert"
        android:layout_marginRight="@dimen/new_137px"
        android:layout_marginLeft="@dimen/new_137px"
        />
    <Button
        android:id="@+id/btn_next"
        android:layout_width="@dimen/new_806px"
        android:layout_height="@dimen/new_120px"
        android:enabled="false"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="@dimen/new_915px"
        android:text="确定并登陆"
        android:background="@mipmap/bg_btn_login"
        android:textColor="@color/white"
        android:textSize="@dimen/new_48px"
        tools:ignore="MissingConstraints"
        android:layout_marginRight="@dimen/new_137px"
        android:layout_marginLeft="@dimen/new_137px"
        />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="@dimen/new_299px"
        android:layout_marginRight="@dimen/new_299px"
        android:layout_marginBottom="@dimen/new_43px"
        android:orientation="vertical"
        android:gravity="center"
        tools:ignore="MissingConstraints">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/login_agree"
            android:textSize="@dimen/new_32px"
            android:textColor="@color/cAAAAAA"
            />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/softwareservice"
                android:textColor="@color/c00AC38"
                android:textSize="@dimen/new_32px"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="《隐私协议》"
                android:textColor="@color/c00AC38"
                android:textSize="@dimen/new_32px"
                />
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
